<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.authine.cloudpivot.ext.mapper.DictMapper">
    <select id="getUserDict" resultType="com.authine.cloudpivot.ext.entity.Dict">
        select id as code, name, imgUrl as link
        from h_org_user
    </select>

    <select id="getDepartmentDict" resultType="com.authine.cloudpivot.ext.entity.Dict">
        select id as code, name
        from h_org_department
    </select>
</mapper>
